{
    "plugins": ["./node_modules/jsdoc-export-default-interop/dist/index.js"],
    "recurseDepth": 10,
    "source": {
        "include": ["package.json", "README.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "opts": {
        "destination": "./documentation/",
        "encoding": "utf8",
        "verbose": true,
        "recurse": true,
        "sort": false,
        "template": "./src/JSdoc"
    }
}
